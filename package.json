{
  "name": "@lifeforge/lifeforge--railway-map",
  "displayName": "Railway Map",
  "repository": {
    "type": "git",
    "url": "https://github.com/lifeforge-app/lifeforge-module-railwayMap.git"
  },
  "version": "0.0.4",
  "author": "Melvin Chia <melvinchia623600@gmail.com>",
  "description": "The ultimate map for MRT and LRT in Malaysia and Singapore",
  "scripts": {
    "types": "cd client && bun tsc",
    "build:client": "cd client && bun run vite build",
    "build:server": "bun build ./server/index.ts --outdir ./server/dist --target bun --external @lifeforge/server-utils --external zod"
  },
  "dependencies": {
    "d3": "^7.9.0",
    "leaflet": "^1.9.4"
  },
  "devDependencies": {
    "@types/d3": "^7.4.3",
    "@types/leaflet": "^1.9.20"
  },
  "lifeforge": {
    "icon": "uil:subway",
    "category": "Information"
  },
  "peerDependencies": {
    "@lifeforge/server-utils": "workspace:*",
    "lifeforge-ui": "workspace:*",
    "shared": "workspace:*"
  }
}
